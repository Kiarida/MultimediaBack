<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Interactions" table="interactions">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateheure" type="datetime" column="dateHeure" nullable="false"/>
    <many-to-one field="idecoute" target-entity="Ecoute">
      <join-columns>
        <join-column name="idEcoute" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idtypeinteraction" target-entity="Typeinteraction">
      <join-columns>
        <join-column name="idTypeInteraction" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
